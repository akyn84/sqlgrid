{
  "author": "Lubomir Andrisek",
  "dependencies": {
    "bootstrap": "^3.4.1",
    "font-awesome": "^4.7.0",
    "jquery": "^3.4.0",
    "jquery-ui": "^1.12.1"
  },
  "description": "react templating system",
  "devDependencies": {
    "@babel/core": "^7.4.0",
    "@babel/plugin-proposal-class-properties": "^7.4.0",
    "@babel/preset-env": "^7.4.2",
    "@babel/preset-react": "^7.0.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^23.6.0",
    "babel-loader": "^8.0.6",
    "babel-preset-env": "^1.7.0",
    "classnames": "^2.2.6",
    "css-loader": "^2.1.1",
    "jest": "^23.6.0",
    "npm-watch": "^0.5.0",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "react-drag-and-drop": "^3.0.0",
    "react-dropzone": "^8.0.3",
    "style-loader": "^0.23.1",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.1.2",
    "webpack-merge": "^4.2.1"
  },
  "jest": {
    "automock": false,
    "moduleFileExtensions": [
      "js",
      "json",
      "jsx"
    ],
    "moduleDirectories": [
      "node_modules"
    ],
    "roots": [
      "__tests__"
    ],
    "testRegex": "(/src/__tests__/.*|\\.(test|spec))\\.(js|jsx)$",
    "unmockedModulePathPatterns": [
      "<rootDir>/node_modules/react/",
      "<rootDir>/node_modules/react-dom/"
    ],
    "setupFiles": [
      "./src/__tests__/config.js"
    ]
  },
  "keywords": [
    "template",
    "react"
  ],
  "name": "npm-masala",
  "version": "1.0.1",
  "scripts": {
    "test": "jest"
  }
}
